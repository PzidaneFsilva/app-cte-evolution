{
  "expo": {
    "name": "Centro de Treinamento Evolution",
    "slug": "app-final",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/minha-logor.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/minha-logor.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Precisamos da sua localização para registrar sua participação nos desafios e validar seus check-ins."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pzidanefsilva.cteapp",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa de acesso à sua galeria para que você possa escolher fotos para seu perfil e postagens.",
          "cameraPermission": "O aplicativo precisa de acesso à sua câmera para que você possa tirar fotos para seu perfil e postagens."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Seu app usa a localização para registrar sua participação nos desafios."
        }
      ]
    ],
    "scheme": "app-final",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "97e80cb0-a064-4d10-adda-b44034048913"
      }
    }
  }
}
